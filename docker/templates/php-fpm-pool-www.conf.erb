[www]

user = <%= config.process_user %>
group = <%= config.process_group %>

listen = <%= config.php_fpm_socket %>

listen.owner = <%= config.process_user %>
listen.group = <%= config.process_group %>
listen.mode = 0660

pm = dynamic

pm.max_children = 50

pm.start_servers = 2
pm.min_spare_servers = 2
pm.max_spare_servers = 4

chdir = /
